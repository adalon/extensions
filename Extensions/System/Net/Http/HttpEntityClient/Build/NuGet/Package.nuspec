<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata>
		<id>netfx-System.Net.Http.HttpEntityClient</id>
		<version>1.1.1.37</version>
		<title>NETFx HttpEntityClient</title>
		<description xml:space="preserve">Strong-typed Linq to Web API. Builds on top of HttpClient and provides the easiest way to interface with typed REST services that are modeled around typed entities or contracts.

Example:
var products = client
	.Query&lt;Product&gt;("products")
	.OrderBy(x => x.Downloads)
	.Skip(25)
	.Take(25)
	.ToList();	
</description>
		<summary>Simple CRUD + Query typed HTTP client</summary>
		<authors>Daniel Cazzulino, kzu, Clarius</authors>
		<language>en-US</language>
		<projectUrl>http://netfx.codeplex.com</projectUrl>
		<licenseUrl>http://netfx.codeplex.com/license</licenseUrl>
		<iconUrl>http://download.codeplex.com/Project/Download/FileDownload.aspx?ProjectName=netfx&amp;DownloadId=199201</iconUrl>
		<tags>netfx http wcf webapi</tags>
		<dependencies>
			<dependency id="HttpClient" version="0.3.0"/>
			<dependency id="Newtonsoft.Json" version="4.0.2" />
			<dependency id="netfx-Reflector" version="1.0.0.7" />
			<dependency id="netfx-System.Net.Http.HttpNameValueCollection" version="1.0.0.23" />
		</dependencies>
		<frameworkAssemblies>
			<frameworkAssembly assemblyName="System.Web" targetFramework="net40"/>
			<frameworkAssembly assemblyName="System.Data.Services.Client" targetFramework="net40"/>
		</frameworkAssemblies>
	</metadata>
</package>